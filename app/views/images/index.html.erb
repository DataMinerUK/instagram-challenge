<% if @images.any? %>
  <% @images.each do |image| %>
    <p><%= link_to image.get_username_of_creator, user_path(image.user_id) %></p>
    <%= link_to image_tag(image.picture.url(:medium), class: 'uploaded-pic medium'), image_path(image) %>
    <p><%= image.description %></p>

  <% end %>
<% else %>
  <h1>No images yet</h1>
<% end %>
